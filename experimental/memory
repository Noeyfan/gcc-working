#include <memory>

namespace std {
namespace experimental {
inline namespace fundamentals_v1 {
  template<typename _Tp>
    class shared_ptr : __shared_ptr<_Tp>
    {
    public:
      constexpr shared_ptr() noexcept = default;

      template<typename _Tp1>
	shared_ptr(_Tp1* __p)
      	: __shared_ptr<_Tp>(__p) { }

      ~shared_ptr() = default;
    };
}
}
}
